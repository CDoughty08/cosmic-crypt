{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "one-line": [
            true,
            "check-open-brace"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            false
        ],
        "max-line-length": [
            true,
            180
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-console": [
            false
        ]
    },
    "rulesDirectory": []
}